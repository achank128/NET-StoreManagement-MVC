@model ExportStore

@{
    ViewData["Title"] = "Delete Export Store";
}

<div class="row text">
    <div class="col-md-8 offset-2">
        <h2>Xác nhận xóa phiếu xuất</h2>
        <div class="row">
            <div>
                <div class="form-group  mt-2 d-flex justify-content-between">
                    <a class="btn btn-outline-secondary" asp-action="Index">Quay lại</a>
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="Id" />
                        <input type="submit" value="Xác nhận" class="btn btn-danger float-right" />
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>